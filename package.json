{
  "name": "vue-custom-fields",
  "version": "2.0.0",
  "description": "Modern Vue 3 plugin for creating custom-styled form elements (selects, checkboxes, file inputs)",
  "main": "./dist/vue-custom-fields.umd.js",
  "module": "./dist/vue-custom-fields.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue-custom-fields.es.js",
      "require": "./dist/vue-custom-fields.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrefigueira/fields.js.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "plugin",
    "custom-fields",
    "form",
    "select",
    "checkbox",
    "file-input",
    "ui",
    "component"
  ],
  "author": "Andr√© Figueira",
  "license": "GPL-3.0-or-later",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vue": "^3.4.0",
    "vue-tsc": "^1.8.0"
  }
}
